package com.kodnest.patterns.level6;

public class Pattern2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char ch = 'A';
		for(int i=1;i<=5;i++) {
			for(int j=1;j<=i;j++) {
				if(j==1) {
					System.out.print(ch);
				}
			}
			System.out.println();
			ch++;
		}
	}

}
